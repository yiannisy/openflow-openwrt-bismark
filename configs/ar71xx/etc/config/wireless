config wifi-device  wifi0
	option type     atheros
	option channel  auto

	# REMOVE THIS LINE TO ENABLE WIFI:
	option disabled 0

config wifi-iface
	option device	wifi0
	option network	lan
	option mode	ap
	option ssid	OpenFlow
	option encryption none

config wifi-device  wifi1
	option type     atheros
	option channel  auto

	# REMOVE THIS LINE TO ENABLE WIFI:
	option disabled 0

config wifi-iface
	option device	wifi1
	option network	lan
	option mode	ap
	option ssid	OpenFlow
	option encryption none
